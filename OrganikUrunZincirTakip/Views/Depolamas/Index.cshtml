@model IEnumerable<OrganikUrunZincirTakip.Models.Depolama>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DepolamaYer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepolamaTarih)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepolamaAcıklama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Denetleme.DenetlemeAcıklama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DepolamaYer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DepolamaTarih)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DepolamaAcıklama)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Denetleme.DenetlemeAcıklama)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Username)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DepolamaID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DepolamaID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DepolamaID })
        </td>
    </tr>
}

</table>
